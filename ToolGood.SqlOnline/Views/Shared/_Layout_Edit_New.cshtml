<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>@ViewBag.Title -后台管理系统</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="format-detection" content="telephone=no">
    <link href="/_/layui/css/layui.css" rel="stylesheet" media="all" />
    <link href="/_/css/global.css" rel="stylesheet" media="all" />
    <link href="/_/css/font-awesome.min.css" rel="stylesheet" />
    <link href="/_/layui/css/modules/layer/default/layer.css" rel="stylesheet" id="layuicss-layer" />
    <style>
        .listDetail { height: 180px; }
        .showDetail { display: none; }
        .showDetailHeader { padding: 10px 0; }
        .showDetailHeaderDetail { width: auto; height: 100%; }
        .closeDetail { margin-top: 40px; }
    </style>
    @RenderSection("css", false)
</head>
<body>
    @RenderBody()
    <script src="/_/js/jquery.min.js"></script>
    <script src="~/_/js/doT.min.js"></script>
    <script src="/_/layui/layui.js"></script>
    <script src="/_/js/global.js"></script>
    <script src="~/_/js/formhelper.js"></script>
    @RenderSection("js", false)
    <div style="display:none">@(ViewData["_test_monitor"]?.ToString().ToHtml())</div>
</body>
</html>